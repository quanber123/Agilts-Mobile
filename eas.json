        "EXPO_PUBLIC_BACKEND_URL": "https://backend.agilts.com",
        "EXPO_PUBLIC_API_COUNTRY": "https://esgoo.net/api-tinhthanh",
        "EXPO_PUBLIC_APP_NAME": "Agilts"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_BACKEND_URL": "https://backend.agilts.com",
        "EXPO_PUBLIC_API_COUNTRY": "https://esgoo.net/api-tinhthanh",
        "EXPO_PUBLIC_APP_NAME": "Agilts"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
